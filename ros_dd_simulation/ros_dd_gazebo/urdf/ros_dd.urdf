<?xml version="1.0" ?>
<robot name="ros_dd_small">
    <link name="base_footprint"/>
    <link name="base_link">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <box size="0.5 0.3 0.15"/>
            </geometry>
            <material name="red">
                <color rgba="1 0 0 1"/>
            </material>
        </visual>
        
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <box size="0.5 0.3 0.15"/>
            </geometry>
        </collision>
        
        <inertial>
            <mass value="3.0"/>
            <origin xyz="0 0 0"/>
            <inertia ixx="0.010" ixy="0.0" ixz="0.0" 
                     iyy="0.010" iyz="0.0" 
                     izz="0.010"/>
        </inertial>
    </link>

    <joint name="base_joint" type="fixed">
        <parent link="base_footprint"/>
        <child link="base_link"/>
        <origin xyz="0 0 0.075" rpy="0 0 0"/>
    </joint>
    

    <joint name="left_wheel_joint" type="continuous">
        <parent link="base_link"/>
        <child link="left_wheel_link" />
        <origin xyz="0 0.175 -0.008" rpy="0 0 0"/>
        <axis xyz="0 1 0" />
    </joint>
    <link name="left_wheel_link">
        <visual>
            <origin rpy="1.570795 0 0" xyz="0 0 0"/>
            <geometry>
                <cylinder length="0.05" radius="0.075"/>
            </geometry>
            <material name="yellow">
                <color rgba="1 1 0 1"/>
            </material>
        </visual>
        <collision>
            <origin rpy="1.570795 0 0" xyz="0 0 0"/>
            <geometry>
                <cylinder length="0.05" radius="0.075"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="0.0625"/>
            <origin rpy="1.570795 0 0" xyz="0 0 0"/>
            <inertia ixx="0.00009" ixy="0.0" ixz="0.0" iyy="0.00009" iyz="0.0" izz="0.00009"/>
        </inertial>
    </link>

    <joint name="right_wheel_joint" type="continuous">
        <parent link="base_link"/>
        <child link="right_wheel_link" />
        <origin xyz="0 -0.175 -0.008" rpy="0 0 0"/>
        <axis xyz="0 1 0" />
    </joint>
    <link name="right_wheel_link">
        <visual>
            <origin rpy="1.570795 0 0" xyz="0 0 0"/>
            <geometry>
                <cylinder length="0.05" radius="0.075"/>
            </geometry>
            <material name="yellow">
                <color rgba="1 1 0 1"/>
            </material>
        </visual>
        <collision>
            <origin rpy="1.570795 0 0" xyz="0 0 0"/>
            <geometry>
                <cylinder length="0.05" radius="0.075"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="0.0625"/>
            <origin rpy="1.570795 0 0" xyz="0 0 0"/>
            <inertia ixx="0.00009" ixy="0.0" ixz="0.0" iyy="0.00009" iyz="0.0" izz="0.00009"/>
        </inertial>
    </link>

    <joint name="caster_front_joint" type="fixed">
        <parent link="base_link"/>
        <child link="caster_link_front"/>
        <origin xyz="0.2 -0.0 -0.005" rpy="0 0 0"/>
    </joint>
    <link name="caster_link_front">
        <visual>
            <geometry><sphere radius="0.03"/></geometry>
            <material name="black"><color rgba="0 0 0 1"/></material>
        </visual>
        <collision>
            <geometry><sphere radius="0.03"/></geometry>
        </collision>
        <inertial>
            <mass value="0.03"/>
            <inertia ixx="0.00002" ixy="0.0" ixz="0.0" iyy="0.00002" iyz="0.0" izz="0.00002"/>
        </inertial>
        <gazebo reference="caster_link_front">
            <mu1>0.0</mu1>
            <mu2>0.0</mu2>
        </gazebo>
    </link>
    
    <joint name="caster_rear_joint" type="fixed">
        <parent link="base_link"/>
        <child link="caster_link_rear"/>
        <origin xyz="-0.2 -0.0 -0.005" rpy="0 0 0"/>
    </joint>
    <link name="caster_link_rear">
        <visual>
            <geometry><sphere radius="0.03"/></geometry>
            <material name="black"><color rgba="0 0 0 1"/></material>
        </visual>
        <collision>
            <geometry><sphere radius="0.03"/></geometry>
        </collision>
        <inertial>
            <mass value="0.03"/>
            <inertia ixx="0.00002" ixy="0.0" ixz="0.0" iyy="0.00002" iyz="0.0" izz="0.00002"/>
        </inertial>
        <gazebo reference="caster_link_rear">
            <mu1>0.0</mu1>
            <mu2>0.0</mu2>
        </gazebo>
    </link>

    <joint name="laser_joint" type="fixed">
        <origin xyz="0.3 0 0.15" rpy="0 0 0"/>
        <parent link="base_link"/>
        <child link="laser_link"/>
    </joint>
    
    <link name="laser_link">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <cylinder length="0.05" radius="0.025"/>
            </geometry>
            <material name="black">
                <color rgba="1 0 0 1"/>
            </material>
        </visual>
        
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <cylinder length="0.05" radius="0.025"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="0.05"/>
            <origin xyz="0 0 0"/>
            <inertia ixx="0.000006" ixy="0.0" ixz="0.0" iyy="0.000006" iyz="0.0" izz="0.000003"/>
        </inertial>
    </link>

     <gazebo>
        <plugin name="differential_drive_controller" filename="libgazebo_ros_diff_drive.so">
            <legacyMode>false</legacyMode>
            
            <left_joint>left_wheel_joint</left_joint> 
            <right_joint>right_wheel_joint</right_joint> 
            
            <wheel_separation>0.35</wheel_separation>
             <wheel_diameter>0.15</wheel_diameter> 
            
            <ros>
                <namespace></namespace>
                <remapping>cmd_vel:=cmd_vel</remapping>
                <remapping>odom:=odom</remapping>
            </ros>
            <odometry_topic>odom</odometry_topic>
            <odometry_frame>odom</odometry_frame>
            <robot_base_frame>base_footprint</robot_base_frame>
            <publish_odom>true</publish_odom>
            <publish_odom_tf>true</publish_odom_tf>
            
            <max_wheel_torque>20</max_wheel_torque>
            <max_wheel_acceleration>1.0</max_wheel_acceleration>
        </plugin>
    </gazebo>
    <gazebo reference="laser_link">
        <sensor type="ray" name="laser_sensor">
            <pose>0 0 0 0 0 0</pose>
            <visualize>true</visualize>
            <update_rate>10</update_rate>
            <alwaysOn>true</alwaysOn>
            <ray>
                <scan>
                    <horizontal>
                        <samples>360</samples>
                        <resolution>1</resolution>
                        <min_angle>-3.141592</min_angle> <max_angle>3.141592</max_angle> 
                    </horizontal>
                </scan>
                <range>
                    <min>0.05</min>
                    <max>8.0</max>
                    <resolution>0.01</resolution>
                </range>
                <noise>
                    <type>gaussian</type>
                    <mean>0.0</mean>
                    <stddev>0.01</stddev>
                </noise>
            </ray>
            <plugin name="gazebo_ros_laser_controller" filename="libgazebo_ros_ray_sensor.so">
                <ros>
                    <namespace></namespace>
                    <remapping>~/out:=scan</remapping> 
                </ros>
                <output_type>sensor_msgs/LaserScan</output_type>
                <frame_name>laser_link</frame_name>
            </plugin>
        </sensor>
    </gazebo>

    <gazebo>
        <plugin name="gazebo_ros_joint_state_publisher" 
                filename="libgazebo_ros_joint_state_publisher.so">
            <ros>
                <namespace></namespace>
                <remapping>~/out:=joint_states</remapping>
            </ros>
            <update_rate>50</update_rate>
            <joint_name>left_wheel_joint</joint_name>
            <joint_name>right_wheel_joint</joint_name>
        </plugin>
    </gazebo>

    <gazebo>
        <plugin name="imu_plugin" filename="libgazebo_ros_imu.so">
            <alwaysOn>true</alwaysOn>
            <updateRate>100.0</updateRate>
            <bodyName>base_link</bodyName>
            <topicName>imu/data</topicName>
            <gaussianNoise>0.005</gaussianNoise>
            <xyzOffset>0 0 0</xyzOffset> <rpyOffset>0 0 0</rpyOffset>
        </plugin>
    </gazebo>

    <gazebo reference="base_link">
        <material>Gazebo/Red</material>
        <self_collide>true</self_collide>
    </gazebo>

   
</robot>